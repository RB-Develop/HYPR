{
  "printWidth": 80,
  "arrowParens": "always",
  "singleQuote": true,
  "jsxSingleQuote": false,
  "useTabs": false,
  "tabWidth": 2,
  "trailingComma": "all",
  "semi": true,
  "proseWrap": "always",
  "bracketSpacing": true,
  "bracketSameLine": false,
  "plugins": ["prettier-plugin-tailwindcss"],
  "tailwindFunctions": ["tva"]
}
